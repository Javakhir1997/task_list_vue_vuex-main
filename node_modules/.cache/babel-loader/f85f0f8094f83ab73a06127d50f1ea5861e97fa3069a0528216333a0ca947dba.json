Index', index);\n    },\n\n    isDone() {\n      this.$store.commit('downTodo', this.todo)\n    },\n  }\n\n}\n</script>\n\n<style lang=\"scss\">\nli {\n  text-decoration: none;\n  list-style: none;\n  color: white;\n}\n\n.item-todo {\n  width: 800px;\n  margin: 10px 0;\n  padding: 14px 20px;\n  border-radius: 10px;\n  background: #3d4756;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n\n  &.active {\n    opacity: 0.5;\n  }\n\n  .todo-index-number {\n    flex-basis: 5%;\n  }\n\n  .todo-message {\n    flex-basis: 65%;\n    text-align: start;\n    font-size: 14px;\n    max-lines: 1;\n    overflow: hidden;\n    padding-left: 10px;\n\n    &.active {\n      text-decoration: line-through;\n\n    }\n  }\n\n  .todo-date-icons {\n    flex-basis: 30%;\n    display: flex;\n    align-items: center;\n\n    .todo-date {\n      color: #a4a4a4;\n      font-size: 12px;\n    }\n\n    .icon-delete-edit {\n      display: flex;\n      align-items: center;\n      cursor: pointer;\n\n      .button-edit {\n        margin: 0 20px 0;\n        width: 70px;\n        padding: 6px;\n        font-size: 14px;\n        border-radius: 8px;\n        background-color: green;\n        cursor: pointer;\n      }\n    }\n  }\n\n\n}\n\n\n\n\n@media screen and (min-width: 645px) {\n  li {\n    text-decoration: none;\n    list-style: none;\n    color: white;\n  }\n\n  .item-todo {\n    // width: 800px;\n    margin: 10px 0;\n    padding: 14px 20px;\n    border-radius: 10px;\n    background: #3d4756;\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n\n    &.active {\n      opacity: 0.5;\n    }\n\n    .todo-index-number {\n      flex-basis: 5%;\n    }\n\n    .todo-message {\n      flex-basis: 55%;\n      text-align: start;\n      font-size: 14px;\n      max-lines: 1;\n      overflow: hidden;\n      padding-left: 10px;\n\n      &.active {\n        text-decoration: line-through;\n\n      }\n    }\n\n    .todo-date-icons {\n      flex-basis: 40%;\n   